{
  "name": "@inkbeard/ui-vue",
  "type": "module",
  "version": "0.5.0",
  "main": "./index.ts",
  "files": [
    "assets"
  ],
  "exports": {
    ".": "./index.ts",
    "./changelog.md": "./CHANGELOG.md",
