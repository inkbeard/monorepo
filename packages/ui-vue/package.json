    "@tsconfig/node18": "^18.2.3",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.30",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.0",
    "typescript": "5.2.2",
    "vite": "^5.2.2",
